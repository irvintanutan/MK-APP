<PreferenceScreen xmlns:app="http://schemas.android.com/apk/res-auto">

    <PreferenceCategory app:title="Inventory Setting">


        <SwitchPreferenceCompat
            app:key="trackInventory"
            app:defaultValue="true"
            app:summaryOff="Don't track Inventory when doing Sales Invoice"
            app:summaryOn="Track Inventory when doing Sales Invoice"
            app:title="Track Inventory" />

    </PreferenceCategory>

    <PreferenceCategory app:title="License Setting">


        <SwitchPreferenceCompat
            app:key="license"
            app:defaultValue="false"
            app:enabled="false"
            app:summaryOff="License is not active please activate your account"
            app:summaryOn="License Activated"
            app:title="License Status" />

    </PreferenceCategory>

</PreferenceScreen>
